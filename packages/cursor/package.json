{
  "name": "nexus-cursor",
  "displayName": "Nexus Workspaces",
  "description": "Cursor IDE extension for Nexus workspace management",
  "version": "1.0.0",
  "publisher": "nexus",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other",
    "Workspaces"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "nexus.workspaceList",
        "title": "Nexus: List Workspaces"
      },
      {
        "command": "nexus.workspaceCreate",
        "title": "Nexus: Create Workspace"
      },
      {
        "command": "nexus.workspaceUse",
        "title": "Nexus: Switch Workspace"
      },
      {
        "command": "nexus.workspaceStatus",
        "title": "Nexus: Workspace Status"
      },
      {
        "command": "nexus.workspaceOpen",
        "title": "Nexus: Open Workspace"
      },
      {
        "command": "nexus.workspaceDelete",
        "title": "Nexus: Delete Workspace"
      },
      {
        "command": "nexus.syncStatus",
        "title": "Nexus: Sync Status"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "nexus.workspaceList",
          "when": "true"
        },
        {
          "command": "nexus.workspaceCreate",
          "when": "true"
        },
        {
          "command": "nexus.workspaceUse",
          "when": "true"
        },
        {
          "command": "nexus.workspaceStatus",
          "when": "true"
        },
        {
          "command": "nexus.workspaceOpen",
          "when": "true"
        },
        {
          "command": "nexus.workspaceDelete",
          "when": "true"
        },
        {
          "command": "nexus.syncStatus",
          "when": "true"
        }
      ]
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.0.0",
    "jest": "^29.7.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "nexus",
    "cursor",
    "workspace",
    "extension"
  ],
  "author": "Nexus Team",
  "license": "MIT"
}
