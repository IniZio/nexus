  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vitest": "^2.0.0",
    "@types/node": "^20.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Nexus Team",
  "license": "MIT"
