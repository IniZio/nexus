{
  "name": "@nexus/workspace-docker",
  "version": "0.1.0",
  "description": "Docker backend for Nexus workspace management",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
